<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:width="@dimen/fab_size" android:height="@dimen/fab_size">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/fab_radius"/>
            <gradient
                android:angle="-45"
                android:startColor="#78ffd6"
                android:endColor="#a8ff78"
                android:gradientRadius="360"
                android:type="linear" />
        </shape>
    </item>

    <item android:gravity="center" android:drawable="@drawable/ic_save"
        android:width="24dp" android:height="24dp"/>
</layer-list>